{
  "manifest_version": 3,
  "name": "Auto Tab",
  "version": "1.3",
  "description": "Activates all tabs. Scrolls to all tabs in a window. Scrolls to specific positions.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_icon": {
      "16": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["*://*/*"]
}
